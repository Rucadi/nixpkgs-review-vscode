{
  "name": "nixpkgs-review",
  "displayName": "nixpkgs-review",
  "description": "unofficial Nixpkgs-review vscode integration",
  "publisher": "rucadi",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
   "repository": {
    "type": "git",
    "url": "https://github.com/Rucadi/nixpkgs-review-vscode.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nixpkgs-review-helper.reviewPR",
        "title": "Nixpkgs Review: Review PR"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "ts-node": "^10.0.0",
    "typescript": "^5.8.3"
  }
}
